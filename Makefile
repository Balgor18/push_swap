NAME = push_swap

CC=gcc

CFLAGS=-Wall -Wextra -Werror

RM=rm -f

SRC =	color.c\
		error.c\
		find_min.c \
		ft_pa.c\
		ft_pb.c\
		ft_print.c\
		ft_rr.c\
		ft_rrr.c\
		ft_ss.c\
		init_struct.c\
		push_swap.c\
		solver.c\
		sort_3.c\
		sort_100.c\
		sort_500.c\
		sort_tab.c\
		verif.c\
		while_pb.c

OBJ = $(SRC:.c=.o)

$(NAME): libft $(OBJ)
			@$(CC) $(CFLAGS) -o $@ $(OBJ) -Llibft -lft
